<h1>Figures</h1>

<% @figures.each do |figure| %>
  <h2><a href="/figures/<%=figure.id%>"><%=figure.name%></a></h2><br>
  <%end%>

<h2>Titles</h2>
  <% @titles.each do |title| %>
  <p><%=title.name%></p>
<%end%>

<h2>Landmark</h2>
  <% @lanmarkss.each do |landmark| %>
  <p><%=landmark.name%></p>
<%end%>
